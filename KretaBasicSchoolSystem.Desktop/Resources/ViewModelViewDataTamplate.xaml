                    >
    <DataTemplate DataType="{x:Type controlPanelViewModels:ControlPanelViewModel}">
        <controlPanelView:ControlPanelView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolCitizensViewModels:SchoolCitizensViewModel}">
        <schoolCitizensView:SchoolCitizensView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolCitizensViewModels:StudentViewModel}">
        <schoolCitizensView:StudentView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolClassesViewModel:SchoolClassesViewModel}">
        <schoolClassesView:SchoolClassesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolSubjectsViewModel:SchoolSubjectsViewModel}">
        <schoolSubjectsView:SchoolSubjectsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolCitizensViewModels:TeacherViewModel}">
        <schoolCitizensView:TeacherView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolCitizensViewModels:ParentViewModel}">
        <schoolCitizensView:ParentView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolOsztalyzatokViewModel:SchoolOsztalyzatokViewModel}">
        <schoolOsztalyzatokView:SchoolOsztalyzatokView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolOsztalyzatokViewModel:AktualisOraViewModel}">
        <schoolOsztalyzatokView:AktualisOraView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolOsztalyzatokViewModel:EvVegeZarasViewModel}">
        <schoolOsztalyzatokView:EvVegeZarasView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolOsztalyzatokViewModel:FelevZarasViewModel}">
        <schoolOsztalyzatokView:FelevZarasView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type schoolOsztalyzatokViewModel:TanitottOsztalyokViewModel}">
